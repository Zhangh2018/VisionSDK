<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="property_sheets_vs" />
  <ImportGroup Label="PropertySheets">
    <Import Project="common.props" />
    <Import Project="APU_Lib.props" />
    <Import Project="oal.props" />
  </ImportGroup>
  <PropertyGroup Label="UserMacros">
    <ACFLibInstall>$(MyProjectRoot)\tools\emu\acf</ACFLibInstall>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <ClCompile>
      <AdditionalIncludeDirectories>$(ACFLibInstall)\src;%(AdditionalIncludeDirectories)</AdditionalIncludeDirectories>
      <PreprocessorDefinitions>APEX2_EMULATE;ACF_KERNEL_METADATA;ACF_KERNEL_IMPLEMENTATION;_MBCS;%(PreprocessorDefinitions)</PreprocessorDefinitions>
    </ClCompile>
    <Link>
      <AdditionalLibraryDirectories>%(AdditionalLibraryDirectories)</AdditionalLibraryDirectories>
    </Link>
    <ProjectReference>
      <UseLibraryDependencyInputs>true</UseLibraryDependencyInputs>
    </ProjectReference>
  </ItemDefinitionGroup>
  <ItemGroup>
    <BuildMacro Include="ACFLibInstall">
      <Value>$(ACFLibInstall)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>